import ActiveModel, { IModelConfig } from '@/lib/ActiveModel';

export class <%= params.pathClassName %> extends ActiveModel<I<%= params.className %>> {
  constructor(config?: IModelConfig) {
    super({
      namespace: '<%= params.namespace %>',
      name: '<%= params.fileName %>',
  <% if (shallow) { -%>
    mode: 'shallow',
  <% } else if (single) { %>
    mode: 'single',
  <% } -%>
    ...config,
    });
  }
}
